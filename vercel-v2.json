{
  "name": "grantsea-website-v2",
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "cleanUrls": true,
  "trailingSlash": false,
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "enabled": true,
    "autoAlias": false
  },
  "functions": {
    "src/app/api/appraisal/route.ts": {
      "maxDuration": 10
    },
    "src/app/api/contact/route.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "FORCE_DEPLOYMENT": "v2-full-screen-fix"
  }
}