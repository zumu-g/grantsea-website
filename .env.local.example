# CRM API Configuration
# Copy this file to .env.local and add your actual API credentials

# Your CRM API base URL
NEXT_PUBLIC_CRM_API_URL=https://api.yourcrm.com/v1

# Your CRM API key
NEXT_PUBLIC_CRM_API_KEY=your_api_key_here

# Optional: API timeout in milliseconds
NEXT_PUBLIC_API_TIMEOUT=30000

# Optional: Enable API caching
NEXT_PUBLIC_ENABLE_API_CACHE=true